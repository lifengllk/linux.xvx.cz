
config global
	option anon_swap '0'
	option anon_mount '0'
	option auto_swap '1'
	option auto_mount '1'
	option delay_root '5'
	option check_fs '0'

config mount
	option target '/mnt/sda1'
	option uuid 'af56-44ae'
	option enabled '0'

config mount
	option target '/mnt/sdb1'
	option uuid '0012-d687'
	option enabled '0'

config mount
	option device '/dev/mmcblk0p1'
	option fstype 'ext4'
	option options 'rw,sync,noatime,nodiratime'
	option enabled '1'
	option enabled_fsck '0'
	option target '/data'

